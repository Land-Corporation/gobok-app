{
  "name": "land-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appId=io.landcorp.app.debug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-dep": "yarn upgrade-interactive --latest",
    "cp-a": "appcenter codepush release-react -a Land-Corporation/land-app-android -d Production",
    "cp-i": "appcenter codepush release-react -a Land-Corporation/land-app-ios -d Production",
    "cp-ls-a": "appcenter codepush deployment history -a Land-Corporation/land-app-android Production",
    "cp-ls-i": "appcenter codepush deployment history -a Land-Corporation/land-app-ios Production",
    "sm-a": "scripts/upload-srcmap-android.sh",
    "sm-i": "scripts/upload-srcmap-ios.sh",
    "bundle-a": "android/gradlew -p android/app bundleRelease && cp android/app/build/outputs/bundle/release/app-release.aab $HOME/Desktop/$(date '+%y%m%d_%H%M%S').aab",
    "install-a": "android/gradlew -p android/app installRelease",
    "assemble-a": "android/gradlew -p android/app assembleRelease && cp android/app/build/outputs/apk/release/app-release.apk $HOME/Desktop/$(date '+%y%m%d_%H%M%S').apk",
    "ver-up": "scripts/increment-version.sh"
  },
  "dependencies": {
    "@bugsnag/react-native": "7.5.5",
    "@react-native-async-storage/async-storage": "1.13.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.11.2",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "amplitude-js": "7.3.3",
    "appcenter": "4.0.2",
    "appcenter-analytics": "4.0.2",
    "appcenter-crashes": "4.0.2",
    "jwt-decode": "3.1.2",
    "mobx": "5.x",
    "mobx-react": "6.x",
    "moment": "2.29.1",
    "nanoevents": "5.1.10",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-autolink": "3.0.0",
    "react-native-code-push": "7.0.0",
    "react-native-device-info": "7.3.1",
    "react-native-gesture-handler": "1.9.0",
    "react-native-image-crop-picker": "0.35.2",
    "react-native-image-zoom-viewer": "3.0.1",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-raw-bottom-sheet": "2.2.0",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.16.1",
    "react-native-url-polyfill": "1.2.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/amplitude-js": "^7.0.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.7",
    "@types/styled-components-react-native": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
